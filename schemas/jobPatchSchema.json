{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "_token": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJ1c2VybmFtZSI6ImpvczEzMzc2IiwiaXNfYWRtaW4iOnRydWUsImlhdCI6MTU5MjY2NTI2MH0._TT-i6gFE1AwX9jqyOYI_EoHP2WhGVLDw53gMqki-U8",
            "title": "Janitor",
            "salary": 85000.0,
            "equity": 150,
            "company_handle": "apple"
        }
    ],
    "required": [
        "_token"
    ],
    "additionalProperties": false,
    "properties": {
        "_token": {
            "$id": "#/properties/_token",
            "type": "string",
            "title": "The _token schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJ1c2VybmFtZSI6ImpvczEzMzc2IiwiaXNfYWRtaW4iOnRydWUsImlhdCI6MTU5MjY2NTI2MH0._TT-i6gFE1AwX9jqyOYI_EoHP2WhGVLDw53gMqki-U8"
            ]
        },
        "title": {
            "$id": "#/properties/title",
            "type": "string",
            "title": "The title schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Janitor"
            ]
        },
        "salary": {
            "$id": "#/properties/salary",
            "type": "number",
            "title": "The salary schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0.0,
            "examples": [
                85000.0
            ]
        },
        "equity": {
            "$id": "#/properties/equity",
            "type": "integer",
            "title": "The equity schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                150
            ]
        },
        "company_handle": {
            "$id": "#/properties/company_handle",
            "type": "string",
            "title": "The company_handle schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "apple"
            ]
        }
    }
}