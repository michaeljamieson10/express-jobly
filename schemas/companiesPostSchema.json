{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "_token": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJ1c2VybmFtZSI6ImpvczEzMzc2IiwiaXNfYWRtaW4iOnRydWUsImlhdCI6MTU5MjY2NTI2MH0._TT-i6gFE1AwX9jqyOYI_EoHP2WhGVLDw53gMqki-U8",
            "handle": "Windowsesd",
            "name": "Bill Gatess",
            "num_employees": 5000,
            "description": "maker of microsoft",
            "logo_url": "https://www.shutterstock.com/image-vector/link-unlink-icons-lock-unlock-chain-1021581091"
        }
    ],
    
    "required": [
        "_token",
        "handle",
        "name",
        "num_employees",
        "description",
        "logo_url"
