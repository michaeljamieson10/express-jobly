    "additionalProperties": false,
    "properties": {
        "_token": {
            "$id": "#/properties/_token",
            "type": "string",
            "title": "The _token schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJ1c2VybmFtZSI6ImpvczEzMzc2IiwiaXNfYWRtaW4iOnRydWUsImlhdCI6MTU5MjY2NTI2MH0._TT-i6gFE1AwX9jqyOYI_EoHP2WhGVLDw53gMqki-U8"
            ]
        },
        "handle": {
            "$id": "#/properties/handle",
            "type": "string",
            "title": "The handle schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Windowsesd"
            ]
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Bill Gatess"
            ]
        },
        "num_employees": {
            "$id": "#/properties/num_employees",
            "type": "integer",
            "title": "The num_employees schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                5000
            ]
        },
        "description": {
            "$id": "#/properties/description",
            "type": "string",
            "title": "The description schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "maker of microsoft"
            ]
        },
        "logo_url": {
            "$id": "#/properties/logo_url",
            "type": "string",
            "title": "The logo_url schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "https://www.shutterstock.com/image-vector/link-unlink-icons-lock-unlock-chain-1021581091"
            ]
        }
    }
}